				; translating the 2D coordinates into a single 1D index with
				; formula: current_row * total_columns + current_column

				; setting the maze
 000003A2			buildmaze proc
 000003A2  B9 00000015		    mov ecx, rows   ; keeping track of how many rows to fill
 000003A7  BE 00000000		    mov esi, 0      ; current row number

 000003AC			    nextrow:
 000003AC  BF 00000000 R	        mov edi, offset maze         ; edi will be storing the current column = 0 in the beginning
 000003B1  8B C6		        mov eax, esi                 ; esi stores the row index
 000003B3  6B C0 38		        imul eax, cols + 1           ; doing current_row * total_columns
 000003B6  03 F8		        add edi, eax                 ; doing + current_column and getting the 2d index at edi 

				        ; total columns
 000003B8  BB 00000037		        mov ebx, cols

				        ; comparing row number whether to draw boundary or space
 000003BD  83 F9 15		        cmp ecx, rows   ; first row
 000003C0  74 09		        je boundary

 000003C2			        second_compare:
 000003C2  83 F9 01		            cmp ecx, 1   ; last row
 000003C5  74 04		            je boundary
				        
 000003C7  B0 2E		        mov al, '.'   ; filling the maze with food
 000003C9  EB 02		        jmp fillrow

 000003CB			        boundary:
 000003CB  B0 23		            mov al, '#'  ; print boundary

				        ; loop to fill the current row
 000003CD			        fillrow:
 000003CD  88 07		            mov byte ptr [edi], al    ; print at the calculated 2d index converted to 1d index
 000003CF  47			            inc edi
 000003D0  4B			            dec ebx                   ; keep track of columns
 000003D1  75 FA		            jnz fillrow               ; keep filling the row until the columns are completed

 000003D3  C6 07 00		            mov byte ptr [edi], 0     ; adding the null terminater at the end of the row

				            ; same thing calculate the index again for the same row
				            ; for Adding side walls for middle rows
 000003D6  BF 00000000 R	            mov edi, offset maze
 000003DB  8B C6		            mov eax, esi               ; same row since esi didn't change
 000003DD  6B C0 38		            imul eax, cols + 1
 000003E0  03 F8		            add edi, eax

				            ; first check if it isn't the top or bottom wall
 000003E2  83 FE 00		            cmp esi, 0
 000003E5  74 0E		            je skiprow
 000003E7  83 FE 14		            cmp esi, rows - 1
 000003EA  74 09		            je skiprow

				            ; print boundary at first col and last col
 000003EC  C6 07 23		            mov byte ptr [edi], '#'
 000003EF  C6 47 36 23		            mov byte ptr [edi + cols - 1], '#'

				            ; jump to next row
 000003F3  EB 00		            jmp skiprow          

 000003F5			            skiprow:
 000003F5  46			                inc esi
 000003F6  E2 B4		    loop nextrow
